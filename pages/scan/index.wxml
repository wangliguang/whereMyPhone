<!--pages/scan/index.wxml-->
<wxs src="../../utils/wxml.wxs" module="tools" />
<view>
 <view class='cell' wx:for="{{dataArray}}" wx:key="index" wx:for-index="index"  wx:for-item="item" >
    <text>平台：{{item.platform}}</text>
    <text>型号：{{item.model}}</text>
    <text>系统：{{item.system}}</text>
 </view>
</view>

<i-button disabled="{{!isLogin}}" class='submitBtn' bind:click="onGetPhone" type="primary">领取手机</i-button>
